

@model Kitap
<div class="container">
    <form method="post" asp-controller="DashBoard" asp-action="KitapDuzenle" asp-area="Management" >
        <div>
            <label class="form-label" asp-for="KitapAdi">
                Kitap Adı
            </label>
            <input type="text" class="form-control" asp-for="KitapAdi" />
        </div>
     <div>
            <label class="form-label" asp-for="Fiyat">
                Fiyat
            </label>
            <input type="text" class="form-control" asp-for="Fiyat" />
            </div>
            <div>
            <label class="form-label" asp-for="KitapResmi">
                Kitap Resmi
            </label>
            <input type="text" class="form-control" asp-for="KitapResmi" />
           
        </div>
        <button type="submit" class="btn btn-default">Kitabı Düzenle</button>




    </form>

</div>